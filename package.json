{
  "name": "glints-internal-tool",
  "version": "1.0.0",
  "description": "A tool for internal use.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "win64": "electron-packager . GlintsInternalTool --platform=win32 --arch=x64 --version=0.36.0 --asar=true",
    "win32": "electron-packager . GlintsInternalTool --platform=win32 --arch=ia32 --version=0.36.0 --asar=true",
    "mac": "electron-packager . GlintsInternalTool --platform=darwin --arch=x64 --version=0.36.0 --asar=true",
    "linux64": "electron-packager . GlintsInternalTool --platform=linux --arch=x64 --version=0.36.0 --asar=true",
    "linux32": "electron-packager . GlintsInternalTool --platform=linux --arch=ia32 --version=0.36.0 --asar=true"
  },
  "keywords": [
    "Glints"
  ],
  "author": "Tong Jieqi <jieqi@glints.com>",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.0"
  }
}